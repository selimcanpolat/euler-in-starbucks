
import pandas as pd

from sklearn.tree import DecisionTreeClassifier

df = pd.read_csv("/Users/selimpolat/Desktop/bitstampUSD_1-min_data_2012-01-01_to_2020-09-14.csv")

X = df.drop(columns=['Open', 'High', 'Low', 'Volume_(BTC)',  'Volume_(Currency)', 'Weighted_Price',"Close"]) #input data
y = df["Close"]

model = DecisionTreeClassifier()
model.fit(X,y)
model.predict([[1535594910]])

# 1535594910
